    "@types/aws-lambda": "^8.10.83",
    "@types/node": "^16.10.3",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "aws-sdk": "^2.1005.0",
    "lamba-local-test": "^0.1.0",
    "lambaa": "^3.1.3"
  }
